{
	"name": "102_GR05_DF_Transformationtoprocessed",
	"properties": {
		"description": "select the right columns",
		"folder": {
			"name": "102_GR05"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "102_GR05_RAWCOUNTRYDIMENSION",
						"type": "DatasetReference"
					},
					"name": "RAWcountry"
				},
				{
					"dataset": {
						"referenceName": "102_GR05_RAWPartnerDIMENSION",
						"type": "DatasetReference"
					},
					"name": "RAWPartner"
				},
				{
					"dataset": {
						"referenceName": "102_GR05_RAWQuoteDim",
						"type": "DatasetReference"
					},
					"name": "RAWQuote"
				},
				{
					"dataset": {
						"referenceName": "102_GR05_RAWFactable",
						"type": "DatasetReference"
					},
					"name": "RawFact"
				},
				{
					"dataset": {
						"referenceName": "102_GR05_RAWDistributorDIMENSION",
						"type": "DatasetReference"
					},
					"name": "RAWDisti"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "102_GR05_DS_Processed_Country",
						"type": "DatasetReference"
					},
					"name": "ProcessedCountry"
				},
				{
					"dataset": {
						"referenceName": "102_GR05_DS_Processed_Partner",
						"type": "DatasetReference"
					},
					"name": "ProcessedPartner"
				},
				{
					"dataset": {
						"referenceName": "102_GR05_DS_Processed_Quote",
						"type": "DatasetReference"
					},
					"name": "ProcessedQuote"
				},
				{
					"dataset": {
						"referenceName": "102_GR05_DS_Processed_Fact",
						"type": "DatasetReference"
					},
					"name": "ProcessedFact"
				},
				{
					"dataset": {
						"referenceName": "102_GR05_DS_Processed_Distributor",
						"type": "DatasetReference"
					},
					"name": "ProcessedDisti"
				}
			],
			"transformations": [
				{
					"name": "selectquote"
				},
				{
					"name": "selectfact"
				},
				{
					"name": "Surrogatekey"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Country ID} as string,",
				"          {Country Name} as string,",
				"          Region as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> RAWcountry",
				"source(output(",
				"          {Reseller name} as string,",
				"          {Country ID} as string,",
				"          {Type of Reseller} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> RAWPartner",
				"source(output(",
				"          {Quote ID} as string,",
				"          {Renewal Assignee} as string,",
				"          {Opportunity Status} as string,",
				"          {Approval Status} as string,",
				"          {Quote Status} as string,",
				"          {Days To PO Received} as string,",
				"          {Sales Owner} as string,",
				"          {Renewal Opportunity: Quote Status} as string,",
				"          {Issue Comments} as string,",
				"          {Renewal Opportunity: Forecast Category} as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: true,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> RAWQuote",
				"source(output(",
				"          {RPM ID} as string,",
				"          {Quote ID} as string,",
				"          {Disti ID} as string,",
				"          {Partner ID} as string,",
				"          {Customer name} as string,",
				"          {Order Due Date} as string,",
				"          {Order Received Date} as string,",
				"          {Quote Value} as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> RawFact",
				"source(output(",
				"          {Reseller name} as string,",
				"          {Country ID} as string,",
				"          {Type of Reseller} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> RAWDisti",
				"RAWQuote select(mapColumn(",
				"          {Quote ID},",
				"          {Renewal Assignee},",
				"          {Opportunity Status},",
				"          {Approval Status},",
				"          {Quote Status},",
				"          {Days To PO Received},",
				"          {Sales Owner},",
				"          {Renewal Opportunity: Quote Status},",
				"          {Issue Comments},",
				"          {Renewal Opportunity: Forecast Category}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectquote",
				"RawFact select(mapColumn(",
				"          {RPM ID},",
				"          {Quote ID},",
				"          {Disti ID},",
				"          {Partner ID},",
				"          {Customer name},",
				"          {Order Due Date},",
				"          {Order Received Date},",
				"          {Quote Value}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectfact",
				"selectfact keyGenerate(output({Fact PK} as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> Surrogatekey",
				"RAWcountry sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {Country ID} as string,",
				"          {Country Name} as string,",
				"          Region as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> ProcessedCountry",
				"RAWPartner sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {Reseller name} as string,",
				"          {Country ID} as string,",
				"          {Type of Reseller} as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> ProcessedPartner",
				"selectquote sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {Quote ID} as string,",
				"          {Renewal Assignee} as string,",
				"          {Opportunity Status} as string,",
				"          {Approval Status} as string,",
				"          {Quote Status} as string,",
				"          {Days To PO Received} as string,",
				"          {Sales Owner} as string,",
				"          {Renewal Opportunity: Quote Status} as string,",
				"          {Issue Comments} as string,",
				"          {Renewal Opportunity: Forecast Category} as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> ProcessedQuote",
				"Surrogatekey sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {RPM ID} as string,",
				"          {Quote ID} as string,",
				"          {Disti ID} as string,",
				"          {Partner ID} as string,",
				"          {Customer name} as string,",
				"          {Order Due Date} as string,",
				"          {Order Received Date} as string,",
				"          {Quote Value} as string,",
				"          {Fact PK} as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> ProcessedFact",
				"RAWDisti sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {Reseller name} as string,",
				"          {Country ID} as string,",
				"          {Type of Reseller} as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> ProcessedDisti"
			]
		}
	}
}